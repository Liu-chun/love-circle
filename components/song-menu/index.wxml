<!--components/song-menu/index.wxml-->
<view class="menu">
  <music-header title="{{title}}" bindmore="handleMoreClick" data-title="{{title}}"></music-header>
  <scroll-view class="menu-item" scroll-x="true">
    <block wx:for="{{menuList}}" wx:key="id">
      <song-menu-v1 item="{{item}}" bindtap="handleItemClick" data-item="{{item}}"></song-menu-v1>
    </block>
  </scroll-view>
</view>